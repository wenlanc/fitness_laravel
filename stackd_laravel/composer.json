{
    "name": "livelyworks/lw-dating",
    "type": "project",
    "description": "LivelyWorks product based on The Laravel Framework.",
    "keywords": [
        "dating"
    ],
    "license": "NA",
    "require": {
        "php": "^7.3|^8.0",
        "box/spout": "^3.3.0",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.4.0",
        "fruitcake/laravel-cors": "^2.0",
        "fzaninotto/faker": "^1.9.2",
        "gettext/gettext": "^5.5.4",
        "gettext/php-scanner": "^1.3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5.1",
        "laravel/cashier": "^13.7",
        "laravel/framework": "^8.49.1",
        "laravel/helpers": "^1.4.1",
        "laravel/socialite": "^5.2.3",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mk-j/php_xlsxwriter": "dev-master",
        "paypal/paypal-checkout-sdk": "^1.0.1",
        "paypal/rest-api-sdk-php": "^1.14.0",
        "pusher/pusher-php-server": "~7.0.1",
        "razorpay/razorpay": "^2.7.0",
        "silviolleite/laravelpwa": "^2.0",
        "stripe/stripe-php": "^7.87.0",
        "vemcogroup/laravel-sparkpost-driver": "^4.0.4"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Yantrana/Support/php-gettext-1.0.12/gettext.inc",
            "app/Yantrana/Support/translation-helpers.php",
            "app/Yantrana/Support/Agora/RtmTokenBuilder.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
